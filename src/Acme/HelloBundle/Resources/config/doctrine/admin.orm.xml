<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\HelloBundle\Entity\Admin" table="admin">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adminname" type="string" column="adminname" length="100" nullable="false"/>
    <field name="password" type="string" column="password" length="100" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="iplog" type="string" column="iplog" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
