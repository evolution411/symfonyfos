<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\HelloBundle\Entity\Siteinfo" table="siteinfo">
    <id name="siteemail" type="string" column="siteemail" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sitephone" type="string" column="sitephone" length="20" nullable="false"/>
    <field name="aboutus" type="string" column="aboutus" length="250" nullable="false"/>
    <field name="mission" type="string" column="mission" length="250" nullable="false"/>
  </entity>
</doctrine-mapping>
