<div id="maindv-display-image-page-main-body">
 
 
 <form id="regform" action="" method="post">
    
        <h1>You are going to delete this picture for this Apartment.</h1>
        
        <table> 
			<input type="hidden" name="id" value="{{piclist.id}}">
            <tr><td width="250px">Are you sure?</td></tr>
            <tr><td><input class="say" name="posteridtxt" type="hidden" value="{{piclist.posterid}}" /></td></tr>
            <tr><input class="say" name="imgNametxt" type="hidden" value="{{piclist.imgName}}" /></td></tr>
            <tr><td><input class="say" name="pathtxt" type="hidden" value="{{piclist.path}}" /></td></tr>
            <tr><td><input type="hidden" value="0" class="say" name="statustxt"></td></tr>
            <tr><td></td>
				<td>
					<input type="submit" name="deleteConfirmed" class="btnsubmit"  value="Yes, I'm Sure">
					<input type="submit" name="cancel-or-not-deletebtn" class="btnsubmit" value="No, Don't Delete"/>
				</td>
			</tr>
        </table>
</form>

</div>

